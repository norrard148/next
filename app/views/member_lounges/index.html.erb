<p id="notice"><%= notice %></p>

<h1>Member Lounges</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @member_lounges.each do |member_lounge| %>
      <tr>
        <td><%= link_to 'Show', member_lounge %></td>
        <td><%= link_to 'Edit', edit_member_lounge_path(member_lounge) %></td>
        <td><%= link_to 'Destroy', member_lounge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member Lounge', new_member_lounge_path %>
